rmf ${OUTPUT}
a90 = LOAD '${INPUT}' USING PigStorage(',') as (FIELD1:INT, FIELD2:FLOAT, FIELD3:FLOAT, FIELD4:FLOAT, FIELD5:FLOAT, FIELD6:CHARARRAY);

a80_0 = FILTER a90 BY FIELD1 == FIELD1;

a80= FOREACH a80_0 GENERATE FIELD1 AS FIELD1,
       FIELD2 AS FIELD2,
       FIELD3 AS FIELD3,
       FIELD4 AS FIELD4,
       FIELD5 AS FIELD5,
       FIELD6 AS FIELD6;

STORE a80 INTO '${OUTPUT}' USING PigStorage(',');
